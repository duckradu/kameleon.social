{
  "name": "@kameleon.social/web-app",
  "version": "0.0.0",
  "author": "radu.dascalu@pm.me",
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "build-and-migrate": "pnpm build && pnpm db:migrate",
    "start": "vinxi start",
    "db:check": "drizzle-kit check:pg --config=src/server/db/config.ts",
    "db:generate": "drizzle-kit generate:pg --config=src/server/db/config.ts",
    "db:migrate": "node --import tsx --env-file .env ./src/server/db/migrate.ts",
    "db:push": "drizzle-kit push:pg --config=src/server/db/config.ts",
    "db:seed": "node --import tsx --env-file .env tsx ./src/server/db/seed.ts",
    "db:drop": "drizzle-kit drop --config=src/server/db/config.ts",
    "db:studio": "drizzle-kit studio --config=src/server/db/config.ts"
  },
  "dependencies": {
    "@solidjs/meta": "^0.29.3",
    "@solidjs/router": "^0.13.1",
    "@solidjs/start": "^0.7.7",
    "@tiptap/core": "^2.2.4",
    "@tiptap/extension-document": "^2.2.4",
    "@tiptap/extension-hard-break": "^2.2.4",
    "@tiptap/extension-history": "^2.2.4",
    "@tiptap/extension-paragraph": "^2.2.4",
    "@tiptap/extension-placeholder": "^2.2.4",
    "@tiptap/extension-text": "^2.2.4",
    "@tiptap/html": "^2.2.4",
    "@tiptap/pm": "^2.2.4",
    "@unocss/reset": "^0.58.6",
    "@zag-js/avatar": "^0.39.0",
    "@zag-js/menu": "^0.39.0",
    "@zag-js/solid": "^0.39.0",
    "@zag-js/toast": "^0.39.0",
    "@zag-js/dialog": "^0.39.0",
    "argon2": "^0.40.1",
    "date-fns": "^3.6.0",
    "decode-formdata": "^0.6.0",
    "drizzle-orm": "^0.30.4",
    "nanoid": "^5.0.6",
    "postgres": "^3.4.3",
    "qrcode": "^1.5.3",
    "solid-js": "^1.8.16",
    "solid-tiptap": "^0.7.0",
    "tailwind-variants": "^0.2.1",
    "valibot": "^0.30.0",
    "vinxi": "^0.3.10"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.194",
    "@types/qrcode": "^1.5.5",
    "@unocss/preset-web-fonts": "^0.58.6",
    "@unocss/transformer-directives": "^0.58.6",
    "@unocss/transformer-variant-group": "^0.58.6",
    "drizzle-kit": "^0.20.14",
    "pg": "^8.11.3",
    "tsx": "^4.7.1",
    "unocss": "^0.58.6",
    "unocss-preset-animations": "^1.0.1",
    "unplugin-icons": "^0.18.5"
  }
}
